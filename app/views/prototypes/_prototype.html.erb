<div class="card">
  <%= link_to image_tag(prototype.image, class: 'card_imag' ), prototype_path(prototype) %> 
  <div class="card__body">
    <%= link_to prototype.title, prototype_path(prototype.id), class: 'card_title' %>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to "by#{prototype.user.name}", user_path(current_user), class: 'card_user' %> <%#パス未設定%>
  </div>
</div>